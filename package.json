{
  "name": "sails-react",
  "private": true,
  "version": "0.0.0",
  "description": "a Sails application",
  "keywords": [],
  "dependencies": {
    "ejs": "2.3.4",
    "grunt": "1.0.1",
    "grunt-contrib-clean": "1.0.0",
    "grunt-contrib-coffee": "1.0.0",
    "grunt-contrib-concat": "1.0.1",
    "grunt-contrib-copy": "1.0.0",
    "grunt-contrib-cssmin": "1.0.1",
    "grunt-contrib-jst": "1.0.0",
    "grunt-contrib-less": "1.3.0",
    "grunt-contrib-uglify": "1.0.1",
    "grunt-contrib-watch": "1.0.0",
    "grunt-sails-linker": "~0.10.1",
    "grunt-sync": "0.5.2",
    "include-all": "^1.0.0",
    "path": "^0.12.7",
    "rc": "1.0.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-materialize": "^2.1.2",
    "sails": "~0.12.14",
    "sails-disk": "~0.10.9"
  },
  "scripts": {
    "start": "npm-run-all --parallel open:client dev",
    "start:debug": "npm-run-all --parallel open:client debug",
    "open:client": "webpack-dev-server --open",
    "build": "npm run build:prod",
    "build:dev": "webpack",
    "build:prod": "webpack --config assets/config/webpack.prod",
    "clean": "rimraf .tmp && mkdirp .tmp/public",
    "prod": "sails lift --prod",
    "dev": "sails lift --dev",
    "debug": "node --inspect app.js"
  },
  "main": "app.js",
