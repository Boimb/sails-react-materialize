    "css-loader": "^0.28.10",
    "extract-text-webpack-plugin": "^2.1.2",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "raw-loader": "^0.5.1",
